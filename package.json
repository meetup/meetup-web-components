    "sass-loader": "6.0.6",
    "storybook-addon-i18n-tools": "1.0.0",
    "style-loader": "0.20.2",
    "swarm-animation": "0.0.95",
    "swarm-constants": "1.2.80",
    "swarm-icons": "^1.3",
    "swarm-sasstools": "4.0.353-beta",
    "webpack": "2.6.1"
  },
  "lint-staged": {
      "**/*.{js,jsx}": [
        "eslint"
      ]
  }
}
