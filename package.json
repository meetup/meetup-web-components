{
  "name": "meetup-web-components",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "Meetup Web UI Component Library",
  "author": "Meetup",
  "engine": "node >=6.5.0",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx,story.jsx",
    "coverage": "npm run test:unit -- --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "generate": "node src/utils/generate.js",
    "lint": "npm run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "prepublish": "npm run build",
    "storybook": "node src/utils/create-icon-story && start-storybook -p 9001 -c .storybook",
    "test": "npm-run-all --parallel lint:base coverage",
    "test:unit": "jest --silent --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/meetup-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-components/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-components#readme",
  "dependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-jest": "16.0.0",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "classnames": "2.2.5",
    "eslint": "3.9.1",
    "eslint-plugin-react": "6.4.1",
    "meetup-web-platform": "0.5.189",
    "node-sass": "3.10.1",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-intl": "2.1.5",
    "react-transform-catch-errors": "1.0.2",
    "sassquatch2": "2.4.3",
    "swarm-icons": "0.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "2.24.1",
    "babel-loader": "6.2.4",
    "coveralls": "2.11.14",
    "css-loader": "0.23.1",
    "eslint-loader": "1.5.0",
    "file-loader": "0.9.0",
    "inquirer": "1.2.2",
    "jest-cli": "15.1.1",
    "npm-run-all": "2.3.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1"
  }
}
