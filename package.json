{
  "name": "meetup-web-components",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "Meetup Web UI Component Library",
  "author": "Meetup",
  "engine": "node >=6.5.0",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx,story.jsx --source-maps",
    "coveralls": "if [ \"${TRAVIS_PULL_REQUEST}\" = \"false\" ] && [ \"${COVERALLS_REPO_TOKEN}\" != \"\" ]; then cat coverage/lcov.info | coveralls; fi",
    "generate": "node src/utils/generate.js",
    "lint": "yarn run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "postcheckout": "yarn install",
    "postmerge": "yarn install",
    "precommit": "git status --porcelain | awk 'match($1, \"[ACM]\"){print $2}' | grep '[js|jsx]$' | xargs eslint",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-static": "build-storybook -c .storybook -o .storybook-out",
    "gh-pages": "gh-pages -d .storybook-out -b gh-pages",
    "publish-docs": "yarn run storybook-static && yarn run gh-pages",
    "test": "npm-run-all --parallel lint:base test:coverage",
    "test:ci": "yarn lint:base && yarn test:coverage -- --runInBand",
    "test:coverage": "yarn run test:unit -- --coverage",
    "test:base": "jest --color --config jest.config.json",
    "test:unit": "yarn run test:base -- --silent",
    "test:watch": "yarn run test:base -- --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/meetup-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-components/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-components#readme",
  "peerDependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "swarm-icons": "1.x"
  },
  "dependencies": {
    "autosize": "3.0.21",
    "base64-image-loader": "1.2.0",
    "flatpickr": "3.0.6",
    "prop-types": "15.5.10",
    "recompose": "0.24.0",
    "swarm-animation": "https://github.com/meetup/swarm-animation.git"
  },
  "devDependencies": {
    "@alrra/travis-scripts": "3.0.1",
    "@kadira/react-storybook-addon-info": "3.4.0",
    "@kadira/storybook": "2.35.3",
    "@kadira/storybook-addon-knobs": "1.7.1",
    "@kadira/storybook-addon-notes": "1.0.1",
    "@kadira/storybook-addons": "^1.6.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-jest": "19.0.0",
    "babel-loader": "7.1.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "classnames": "2.2.5",
    "coveralls": "2.13.1",
    "css-loader": "0.28.4",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "4.2.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.1.0",
    "gh-pages": "1.0.0",
    "husky": "0.14.3",
    "inquirer": "3.2.0",
    "jest": "20.0.4",
    "meetup-web-mocks": "1.0.194",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "raw-loader": "0.5.1",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-intl": "2.3.0",
    "react-router": "3.0.2",
    "react-transition-group": "2.2.0",
    "sass-loader": "6.0.6",
    "storybook-addon-i18n-tools": "1.0.0",
    "style-loader": "0.18.2",
    "swarm-icons": "1.3.179",
    "swarm-sasstools": "1.7.117",
    "webpack": "2.6.1"
  }
}
