{
  "name": "meetup-web-components",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "Meetup Web UI Component Library",
  "author": "Meetup",
  "engine": "node >=6.5.0",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx,story.jsx",
    "coverage": "npm run test:unit -- --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "generate": "node src/utils/generate.js",
    "lint": "npm run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "npm-run-all --parallel lint:base coverage",
    "test:unit": "jest --silent --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/meetup-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-components/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-components#readme",
  "dependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-jest": "17.0.2",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "classnames": "2.2.5",
    "eslint": "3.11.1",
    "eslint-plugin-react": "6.7.1",
    "meetup-web-mocks": "0.1.10",
    "node-sass": "3.13.0",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-intl": "2.1.5",
    "react-transform-catch-errors": "1.0.2",
    "sassquatch2": "git+https://github.com/meetup/sassquatch2#responsify"
  },
  "devDependencies": {
    "@kadira/storybook": "2.33.1",
    "babel-loader": "6.2.8",
    "coveralls": "2.11.15",
    "css-loader": "0.26.0",
    "eslint-loader": "1.6.1",
    "inquirer": "1.2.3",
    "jest-cli": "17.0.3",
    "npm-run-all": "3.1.2",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "webpack-svgstore-plugin": "3.0.5"
  }
}
