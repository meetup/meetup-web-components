{
  "name": "meetup-web-components",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "Meetup Web UI Component Library",
  "author": "Meetup",
  "engine": "node >=6.5.0",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx,story.jsx --source-maps",
    "coverage": "yarn run test:unit -- --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "generate": "node src/utils/generate.js",
    "lint": "yarn run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "precommit": "git status --porcelain | awk 'match($1, \"[ACM]\"){print $2}' | grep '[js|jsx]$' | xargs eslint",
    "prepublish": "yarn run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "npm-run-all --parallel lint:base coverage",
    "test:unit": "jest --silent --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/meetup-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-components/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-components#readme",
  "peerDependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.3.0",
    "@kadira/storybook": "2.35.3",
    "@kadira/storybook-addon-knobs": "1.7.1",
    "@kadira/storybook-addon-notes": "1.0.1",
    "@kadira/storybook-addons": "^1.6.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.23.0",
    "classnames": "2.2.5",
    "coveralls": "2.12.0",
    "css-loader": "0.26.2",
    "eslint": "3.17.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-react": "6.10.0",
    "husky": "0.13.2",
    "inquirer": "3.0.6",
    "jest-cli": "19.0.2",
    "meetup-web-mocks": "0.1.70",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "raw-loader": "0.5.1",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-intl": "2.2.3",
    "react-router": "4.0.0",
    "sass-loader": "6.0.2",
    "storybook-addon-i18n-tools": "1.0.0",
    "style-loader": "0.13.2",
    "swarm-icons": "0.1.0",
    "swarm-sasstools": "1.0.9",
    "webpack": "^2.2"
  }
}
