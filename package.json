{
  "name": "meetup-web-components",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "Meetup Web UI Component Library",
  "author": "Meetup",
  "engine": "node >=6.5.0",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx,story.jsx",
    "coverage": "npm run test:unit -- --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "generate": "node src/utils/generate.js",
    "lint": "npm run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "npm-run-all --parallel lint:base coverage",
    "test:unit": "jest --silent --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/meetup-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-components/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-components#readme",
  "peerDependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "2.35.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.22.0",
    "babel-jest": "18.0.0",
    "babel-plugin-react-intl": "2.3.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "babel-loader": "6.2.10",
    "classnames": "2.2.5",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "eslint": "3.13.1",
    "eslint-plugin-react": "6.9.0",
    "eslint-loader": "1.6.1",
    "inquirer": "2.0.0",
    "jest-cli": "18.1.0",
    "meetup-web-mocks": "0.1.41",
    "react-addons-test-utils": "15.4.2",
    "node-sass": "4.3.0",
    "npm-run-all": "4.0.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-intl": "2.2.3",
    "sassquatch2": "2.4.4",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "webpack-svgstore-plugin": "3.0.6"
  }
}
